-- Insert sample profiles
INSERT INTO profile (name, username, email, bio) VALUES
('John Doe', 'johndoe', 'john@example.com', 'Hello world!'),
('Jane Smith', 'janesmith', 'jane@example.com', 'Digital creator');

-- Insert sample stories
INSERT INTO stories (profile_id, title, content) VALUES
((SELECT id FROM profile WHERE username = 'johndoe'), 'My First Story', 'This is my first post!'),
((SELECT id FROM profile WHERE username = 'janesmith'), 'Welcome All', 'Great to be here!');